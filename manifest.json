{
  "manifest_version": 3,
  "name": "AI Studio Chat Backup",
  "version": "0.2.0",
  "description": "Back up AI Studio chat logs to TXT with one click.",
  "action": {
    "default_title": "Backup chat"
  },
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://aistudio.google.com/*"],
  "background": {
    "service_worker": "background.js"
  }
}
